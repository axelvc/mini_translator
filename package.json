{
  "name": "mini_translator",
  "visualName": "Mini Translator",
  "description": "Translate a text to a different language",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development run-p dev:*",
    "dev:web": "vite",
    "dev:main": "esno main.ts",
    "build": "NODE_ENV=production run-s build:*",
    "build:web": "vue-tsc --noEmit && vite build",
    "build:main": "esno main.ts",
    "lint": "lint-staged"
  },
  "pre-commit": "lint",
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@floating-ui/dom": "^0.3.1",
    "@vueuse/core": "^8.1.1",
    "vue": "^3.2.25",
    "webextension-polyfill": "^0.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/archiver": "^5.3.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.18",
    "@types/svgo": "^2.6.2",
    "@types/webextension-polyfill": "^0.12.3",
    "@vitejs/plugin-vue": "^2.2.0",
    "archiver": "^5.3.0",
    "chokidar": "^3.5.3",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-vue": "^10.1.5",
    "esno": "^0.14.1",
    "fs-extra": "^10.0.0",
    "globby": "^13.1.1",
    "lint-staged": "^12.3.4",
    "npm-run-all": "^4.1.5",
    "ora": "^6.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "sass": "^1.49.8",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^2.8.0",
    "vite-svg-loader": "^3.1.2",
    "vue-tsc": "^0.29.8"
  }
}
